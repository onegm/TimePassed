from TimePassed import TimeCounter
import time

watch = TimeCounter()

start_time = watch.start()
print(start_time)

time.sleep(7)

stop_time = watch.pause().seconds
print(stop_time)

# time_elapsed = watch.show().seconds
# print(time_elapsed)


start_time = watch.start()
time.sleep(3)
stop_time = watch.stop().seconds
# time_elapsed = watch.show().seconds
print(stop_time)
